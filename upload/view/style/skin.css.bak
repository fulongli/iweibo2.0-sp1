.oauthbtn{background:-moz-linear-gradient(center top,#fff,#e5e3dc);background:-webkit-gradient(linear, left top, left bottom, from(#eee), to(#fff));filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#E5E3DC');border:1px solid #ccc;border-radius:3px;color:#666;color:#f50;}
.oauthbtn:hover{background:-webkit-gradient(linear, left top, left bottom, from(#DCF3FB), to(#efe));border-color:#BDE9FF;color:#229CCD;text-decoration:none;}
.findpaswbtn{background-position:0 -70px;}
.result{border:1px solid #92E1FF;background:#F0FBFF;color:#333;}
.slider3 h2{color:#666;}
.slider3 .btna,.slider3  .btnb{background:url(images/button.gif) no-repeat;}
.slider3 .btna{background-position:-217px -56px;}
.slider3 .btnb{background-position:-239px -56px;}
.facesettingBox .funBox input.btn_cancel{background: -webkit-gradient(linear, left top, left bottom, from(#ABC1D2), to(#9AAFC5));filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ABC1D2', endColorstr='#9AAFC5');}
.alarmtxt{background:-moz-linear-gradient(center top,#E6FDE9,#C9EBCE);background:-webkit-gradient(linear, left top, left bottom, from(#E6FDE9), to(#C9EBCE));filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#E6FDE9', endColorstr='#C9EBCE');background:-o-linear-gradient(top,#E6FDE9,#C9EBCE);border:1px solid #A3DDAB;color:#015813;}
.alarmtxt a{color:#00AA25;font-weight:bold;}
.report_tit{border-bottom:1px dashed #eee;}
.crumb{border-bottom:1px dashed #D8D8D8;}
.crumb .fleft{color:#666;}
.activity .fright h1{color:#62A8D3;}
.activitydes{border:1px dashed #e1e1e1;}
.activitydes p{color:#666;}
.activitylist li p{background:#fff;border:1px solid #E6E6E6;color:#999;}
.activitylist li p strong{color:#000;}
.activitycontent{border:1px solid #E6E6E6;background:#F9F9F9;color:#666;}
.activitymembers li{border-bottom:1px dashed #e1e1e1;color:#666;}
.livewrapper{border:1px solid #F9E4C1;background:#FDF7EC;}
.livebroadcast h1{color:#68AEDC;}
.livebroadcast .fright p{color:#666;}
.livelist li .fright h2 a{color:#68AEDC;}
.livedes{border:1px solid #e1e1e1;background:#fff;}
.livedes strong{color:#666;}
.select .selecttit{background:url(images/select.gif) 0 -23px no-repeat;}
.select  a{color:#909DB0;}
.select  a:hover{color:#666;}
.select ul{background:#fff;border:1px solid #B8C1CA;}
.select ul li a:hover{background-color:#EAECEB;}
.tinterview .askbox{border-color:#FEB470;}
.tinterview .answerbox{border-color:#A5DDF8;}
.tinterview .askbox0,.tinterview .answerbox0{border-color:#fff;border-bottom:1px dashed #e1e1e1;}
.topicform div{background:url(images/searchtopic.gif) no-repeat;}
.topicform div .txt{border:none;}
.topicform div .btn{border:none;color:#666;opacity:0;filter:alpha(opacity=0);background:url(about:blank);}
.topicresult{border:1px solid #e1e1e1;clear:both;}
.topicresult li{border:1px solid #e1e1e1;}
.mainsendboxerror,.mainsendboxsuccess,.minisendboxerror,.minisendboxsuccess{color:#f00;}
.nocollection{background:url(images/nocollection.gif) center top no-repeat;}
.noatme{background:url(images/noatme.gif) center top no-repeat;}
/*皮肤设置*/
.facesettingBox{border:1px solid #8ACAEE;background:#fff;-webkit-box-shadow:0 0 8px rgba(0,0,0,.3);}
.facesettingBox .tit{background: -webkit-gradient(linear, left top, left bottom, from(#fff), to(#CBE8F7));border-bottom:1px solid #8ACAEE;filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#CBE8F7');}
.facesettingBox .tit span{background:url(../images/b3.png) -80px -45px no-repeat;}
.facesettingBox .categorys{border-bottom:1px solid #8ACAEE;position:relative;background:#E7F4FB;}
.facesettingBox .categorys ul li{border-right:1px solid #8ACAEE;}
.facesettingBox .categorys ul li.active{background:#fff;}
.facesettingBox .categorys ul li a:hover{background:#F5FAFD;}
.facesettingBox .facelist li{border:1px solid #eee;}
.facesettingBox .facelist li:hover{border-color:#8ACAEE;}
.facesettingBox .facelist li .mask{background:rgba(0,0,0,.5);filter:alpha(opacity=50);background:#000\9;}
.facesettingBox .facelist li .tempName{color:#fff;}
.facesettingBox .facelist li.locked .ico_lock{background:url(../images/b3.png) -203px -46px no-repeat;}
.facesettingBox .pages strong,.facesettingBox .pages a{border:1px solid #fff;}
.facesettingBox .pages .fright a:hover{border-color:#2B4A78;}
.facesettingBox .funBox{border-top:1px dotted #C8C8C8;}
.activitybtn{background:url(../images/activity.gif) no-repeat;}
.activitybtn:hover{background-position:0 -40px;}
.commendbtn{background:url(../images/commendbtn.gif) no-repeat;}
.commendbtn:hover{background-position:0 -35px;}